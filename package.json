{
  "name": "meet",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "run-s build:react build:postcss",
    "build:react": "webpack --config webpack.prod.js --progress",
    "build:postcss": "postcss ./style/index.css -o ./assets/styles.css",
    "lint": "eslint . --fix",
    "dev": "run-p dev:*",
    "dev:react": "webpack serve --config webpack.dev.js --progress",
    "dev:postcss": "postcss ./style/index.css -o ./assets/styles.css -w",
    "dev:twilio": "twilio-run",
    "deploy": "run-s build deploy:twilio",
    "deploy:twilio": "twilio-run deploy"
  },
  "dependencies": {
    "@twilio/runtime-handler": "1.2.2",
    "cross-env": "^7.0.3",
    "twilio": "^3.56"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@twilio-labs/serverless-runtime-types": "^2.2.2",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "file-loader": "^6.2.0",
    "flowbite": "^1.4.3",
    "html-webpack-plugin": "^5.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tailwindcss": "^3.0.24",
    "ts-loader": "^9.2.9",
    "twilio-run": "^3.4.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "engines": {
    "node": "14"
  }
}
